{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 600,
  "height": 500,
  "title": "Australian States Average Rainfall in 2017 (mm)",
  "projection": {"type": "equirectangular"},
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/EHWS-coding/FIT3179Assignment2/main/data/ausLocalities_with_rain.json",
        "format": {"type": "topojson", "feature": "lga"}
      },
      "mark": {"type": "geoshape", "fill": "#cccccc"},
      "encoding": {
        "tooltip": [
          {
            "field": "properties.STATE",
            "type": "nominal",
            "title": "State"
          },
          {
            "field": "properties.ABB_NAME",
            "type": "nominal",
            "title": "LGA"
          }
        ]
      }
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/EHWS-coding/FIT3179Assignment2/main/data/ausLocalities_with_rain.json",
        "format": {"type": "topojson", "feature": "lga"}
      },
      "mark": {"type": "geoshape"},
      "encoding": {
        "color": {
          "field": "properties.Average_Rain",
          "type": "quantitative",
          "legend": {"type": "gradient", "description": "Average Rainfall (mm)", "orient": "bottom"},
          "scale": {"scheme": "blues"}
        },
        "tooltip": [
          {
            "field": "properties.STATE",
            "type": "nominal",
            "title": "State"
          },
          {
            "field": "properties.ABB_NAME",
            "type": "nominal",
            "title": "LGA"
          },
          {"field": "properties.Average_Rain", "title": "Rainfall (mm)", "type": "quantitative"}
        ]
      }
    }
  ],
  "config": {}
}